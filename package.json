{
  "name": "@siliconmtn/spacelibs-react",
  "version": "0.0.1",
  "description": "Library for reusable React components",
  "main": "index.js",
  "scripts": {
    "build": "npx babel ./src -d ./ --config-file ./.babelrc & npx tsc --rootDir src/ --emitDeclarationOnly",
    "build:watch": "npx babel ./src -w -d ./ --config-file ./.babelrc",
    "buildTypes": "npx tsc --rootDir src/",
    "buildTypes:watch": "npx tsc --watch --rootDir src/ --emitDeclarationOnly",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watchAll ./src/core",
    "test:prod": "jest",
    "buildStyles:watch": "node-sass -w src/core/ -o core/",
    "copyFonts": "cp -R './src/core/layout/SpaceForceLayout/Fonts' './core/layout/SpaceForceLayout'",
    "semantic-release": "semantic-release",
    "docs": "npx jsdoc -c docs.json -r"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smtadmin/spacelibs-react.git"
  },
  "keywords": [
    "space",
    "libs",
    "react"
  ],
  "author": "Silicon Mountain Technologies",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/smtadmin/spacelibs-react/issues"
  },
  "homepage": "https://github.com/smtadmin/spacelibs-react#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "babel-plugin-transform-scss": "^1.0.9",
    "node-sass": "^5.0.0",
    "react-router-dom": "^5.2.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-classes": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@testing-library/react": "^11.2.5",
    "babel-plugin-add-module-exports": "^1.0.4",
    "better-docs": "^2.3.2",
    "eslint": "^7.18.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsdoc": "^31.4.0",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^17.0.1"
  }
}
