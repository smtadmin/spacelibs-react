 {
	"compilerOptions": {
		"noImplicitAny": false,
		"removeComments": true,
		"preserveConstEnums": true,
		"sourceMap": true,
		"declaration": true,
		"outDir": "./",
		"rootDir": "src/",
		"target": "ES5",
		"allowJs": true,
		"skipLibCheck": true
	},
	"exclude": ["node_modules/*", "docs/*", "coverage/*", "./core/*"]
}